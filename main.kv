WindowManager:
    Login:
    SignUp:
    Calc:
    Manager:

<TooltipMDIconButton@MDIconButton+MDTooltip>

<Login>:
    name: "login"
    FloatLayout:
        TooltipMDIconButton:
            icon: "help-circle-outline"
            tooltip_text: "After signing up, use this page to login with your username and password. \nIf you don't have account, switch to the Signup screen to create one."
    MDBoxLayout:
        orientation: "horizontal"
        BoxLayout:
            #elevation: 0
            #md_bg_color: 1,1,1,1
            orientation: "vertical"
            padding: "70dp","160dp", "70dp", "160dp"
            spacing: "40dp"
            MDLabel:
                text: "Login"
                font_size: "40dp"
                bold: True

            BoxLayout:
                orientation: "vertical"
                spacing: "0dp"


                MDCard:
                    orientation: "vertical"
                    padding: "40dp","12dp"
                    height: root.height*0.08
                    BoxLayout:
                        MDLabel:
                            text: "Username"
                            font_size: "18dp"
                        TooltipMDIconButton:
                            icon: "account-circle-outline"
                            tooltip_text: "Type your username here"
                            pos_hint: {"center_x": .5, "center_y": .5}
                    MDTextField:
                        id: LoginUsername
                        line_color_focus: 229/255,60/255,40/255,1
                        font_size: "15dp"


            BoxLayout:
                orientation: "vertical"

                MDCard:
                    orientation: "vertical"
                    padding: "40dp","12dp"
                    BoxLayout:
                        MDLabel:
                            text: "Password"
                            font_size: "18dp"
                        TooltipMDIconButton:
                            id: view
                            icon: "eye-off-outline"
                            tooltip_text: "Toggle Password View"
                            pos_hint: {"center_x": .5, "center_y": .5}
                            on_release:
                                root.password_view(self.icon)

                    MDTextField:
                        id: LoginPassword
                        line_color_focus: 229/255,60/255,40/255,1
                        font_size: "15dp"
                        password: True



            MDRaisedButton:
                text: "Login"
                font_size: "20dp"
                md_bg_color: 229/255,60/255,40/255,1
                text_color: "white"
                pos_hint: {'center_x': .5}
                width: root.width*0.4
                height: root.height*0.07
                size_hint: None, None
                on_release:
                    root.login_validation(LoginUsername.text, LoginPassword.text, root)
                    root.manager.screens[2].ids.my_Label.text = "Welcome " + (root.ids.LoginUsername.text).partition("@")[0]
                    root.manager.transition.direction = "down"
                    root.ids.LoginUsername.text = ""
                    root.ids.LoginPassword.text = ""
                    root.ids.view.icon = "eye-off-outline"
                    root.ids.LoginPassword.password = True

            Button:
                text: "Haven't got an account? Sign Up"
                background_normal: ''
                font_size: "15dp"
                color: 229/255,60/255,40/255,1
                underline: True
                pos_hint: {'center_x': .5}
                width: root.width*0.15
                height: root.height*0.02
                size_hint: None, None
                on_release:
                    app.root.current = "signup"
                    root.manager.transition.direction = "down"
                    root.ids.LoginUsername.text = ""
                    root.ids.LoginPassword.text = ""
                    root.ids.view.icon = "eye-off-outline"
                    root.ids.LoginPassword.password = True


        BoxLayout:
            elevation: 0
            size_hint: 0.8,1
            # md_bg_color: 1,1,1,0
            padding: "100dp"
            canvas.before:
                Rectangle:
                    source: "1x/Artboard 1.png"
                    size: self.size
                    pos: self.pos

<SignUp>:
    name: "signup"
    FloatLayout:
        TooltipMDIconButton:
            icon: "help-circle-outline"
            tooltip_text: "Use this page to create an account,\nif you already have an account switch to the Login screen to log in"
    MDBoxLayout:
        orientation: "horizontal"
        BoxLayout:
            #md_bg_color: 1,1,1,1
            orientation: "vertical"
            padding: "70dp","140dp", "70dp", "120dp"
            spacing: "30dp"
            MDLabel:
                text: "Sign Up"
                font_size: "40dp"
                bold: True
                id: label

            BoxLayout:
                orientation: "vertical"
                spacing: "0dp"


                MDCard:
                    orientation: "vertical"
                    padding: "40dp","12dp"
                    height: root.height*0.08
                    BoxLayout:
                        MDLabel:
                            text: "Username"
                            font_size: "18dp"
                        TooltipMDIconButton:
                            icon: "account-circle-outline"
                            tooltip_text: "Type your username here."
                            pos_hint: {"center_x": .5, "center_y": .5}
                    MDTextField:
                        id: SignUpUsername
                        line_color_focus: 229/255,60/255,40/255,1
                        font_size: "15dp"

            BoxLayout:
                orientation: "vertical"

                MDCard:
                    orientation: "vertical"
                    padding: "40dp","12dp"
                    BoxLayout:
                        MDLabel:
                            text: "Password"
                            font_size: "18dp"
                        TooltipMDIconButton:
                            id: view1
                            icon: "eye-off-outline"
                            tooltip_text: "Toggle Password View"
                            pos_hint: {"center_x": .5, "center_y": .5}
                            on_release:
                                root.password_view_su(self.icon, 1)
                    MDTextField:
                        id: SignUpPassword1
                        line_color_focus: 229/255,60/255,40/255,1
                        font_size: "15dp"
                        password: True

            BoxLayout:
                orientation: "vertical"

                MDCard:
                    orientation: "vertical"
                    padding: "40dp","12dp"
                    BoxLayout:
                        MDLabel:
                            text: "Confirm Password"
                            font_size: "18dp"
                        TooltipMDIconButton:
                            id: view2
                            icon: "eye-off-outline"
                            tooltip_text: "Toggle Password View"
                            pos_hint: {"center_x": .5, "center_y": .5}
                            on_release:
                                root.password_view_su(self.icon, 2)
                    MDTextField:
                        id: SignUpPassword2
                        line_color_focus: 229/255,60/255,40/255,1
                        font_size: "15dp"
                        password: True

            MDRaisedButton:
                text: "Sign Up"
                font_size: "20dp"
                md_bg_color: 229/255,60/255,40/255,1
                text_color: "white"
                pos_hint: {'center_x': .5}
                width: root.width*0.4
                height: root.height*0.07
                size_hint: None, None
                disabled: False
                on_release:
                    # app.root.current = "Calc"
                    # root.manager.transition.direction = "down"
                   # root.signup_validation(SignUpUsername.text, SignUpPassword1.text, root)
                    root.signupbtn(SignUpUsername.text, SignUpPassword1.text, SignUpPassword2.text, root)
                    root.ids.SignUpUsername.text = ""
                    root.ids.SignUpPassword1.text = ""
                    root.ids.SignUpPassword2.text = ""
                    root.ids.view1.icon = "eye-off-outline"
                    root.ids.view2.icon = "eye-off-outline"
                    root.ids.SignUpPassword1.password = True
                    root.ids.SignUpPassword2.password = True

            Button:
                text: "Already have an account? Login"
                background_normal: ''
                background_active: False
                font_size: "15dp"
                color: 229/255,60/255,40/255,1
                underline: True
                pos_hint: {'center_x': .5}
                width: root.width*0.15
                height: root.height*0.02
                size_hint: None, None
                on_release:
                    app.root.current = "login"
                    root.manager.transition.direction = "up"
                    root.ids.SignUpUsername.text = ""
                    root.ids.SignUpPassword1.text = ""
                    root.ids.SignUpPassword2.text = ""
                    root.ids.view1.icon = "eye-off-outline"
                    root.ids.view2.icon = "eye-off-outline"
                    root.ids.SignUpPassword1.password = True
                    root.ids.SignUpPassword2.password = True



        BoxLayout:
            elevation: 0
            size_hint: 0.8,1
            # md_bg_color: 229/255,60/255,40/255,1
            padding: "100dp"
            canvas.before:
                Rectangle:
                    source: "1x/Artboard 2.png"
                    size: self.size
                    pos: self.pos









<Calc>:
    #background_normal: ''
    name: "Calc"
    Screen:
        #background_normal: ''
        ScrollView:
            #background_normal: ''
            scroll_wheel_distance: 80
            effect_cls: 'DampedScrollEffect'
            size: self.size
            canvas.before:
                Color:
                    rgb: 229/255,60/255,40/255,1
                Rectangle:
                    size: self.size

            GridLayout:
                size_hint_y: None
                cols: 1
                padding: "80dp"
                spacing: "40dp"
                height: self.minimum_height
                width: self.minimum_width

##########################################################################################################

                BoxLayout:
                    padding: "50dp","20dp"
                    orientation: "horizontal"
                    Label:
                        size_hint: .6,1
                        text: "Novated Lease"
                        font_name: "font.otf"
                        bold: True
                        font_size: "40dp"
                    Button:
                        text: "History"
                        font_name: "font.otf"
                        bold: True
                        font_size: "25dp"
                        size_hint: .3,None
                        pos_hint: {"center_y": 0.1}
                        height: "50dp"
                        background_color: (0,0,0,0)
                        background_normal: ""
                        canvas.before:
                            Color:
                                rgba: 1,1,1,.2
                            RoundedRectangle:
                                size: self.size
                                pos: self.pos
                                radius: [50]
                        on_release:
                            root.manager_screen()

                    Label:
                        size_hint: 0.5,1

                    Button:
                        text: "Help"
                        font_name: "font.otf"
                        bold: True
                        font_size: "25dp"
                        size_hint: .15,None
                        pos_hint: {"center_y": 0.1}
                        height: "50dp"
                        background_color: (0,0,0,0)
                        background_normal: ""
                        canvas.before:
                            Color:
                                rgba: 1,1,1,.2
                            RoundedRectangle:
                                size: self.size
                                pos: self.pos
                                radius: [50]
                        on_release:
                            root.help_popup()

                    Label:
                        size_hint: 0.05,1

                    Button:
                        text: "Log Out"
                        font_name: "font.otf"
                        bold: True
                        font_size: "25dp"
                        size_hint: .30,None
                        pos_hint: {"center_y": 0.1}
                        height: "50dp"
                        background_color: (0,0,0,0)
                        background_normal: ""
                        canvas.before:
                            Color:
                                rgba: 1,1,1,.2
                            RoundedRectangle:
                                size: self.size
                                pos: self.pos
                                radius: [50]
                        on_release:
                            app.root.current = "login"
                            root.ids.carCost.value = 0
                            root.ids.annualDistance.value = 0
                            root.ids.term1.active = False
                            root.ids.term2.active = False
                            root.ids.term3.active = False
                            root.ids.term4.active = False
                            root.ids.size1.active = False
                            root.ids.size2.active = False
                            root.ids.size3.active = False
                            root.ids.size4.active = False
                            root.ids.preTaxIncome.value = 0
                            root.ids.inc1.active = False
                            root.ids.inc2.active = False
                            root.ids.inc3.active = False
                            root.ids.inc4.active = False



                CalculatorCategory: # Introduction
                    size_hint: 1, None
                    height: "200dp"
                    Title1:
                        id: my_Label
                        text: "hi"
                    Title1:
                        text: "Please select your preferences"
                        font_size: "40dp"
                        bold: False
                        italic: True

                BoxLayout:
                    size_hint: 1,None
                    height: "200dp"
                    spacing: "40dp"

                    CalculatorCategory: # Car Cost
                        height: "250dp"
                        BoxLayout:
                            size_hint: 0.4,1
                            pos_hint: {"center_x": .5}
                            Title2:
                                text: "Car Cost"
                            TooltipMDIconButton:
                                icon: "help-circle-outline"
                                tooltip_text: "Select the approximate value of the car you're purchasing (including GST)"
                                pos_hint: {"center_x": .5, "center_y": .5}
                        MDSlider:
                            hint: False
                            color: 229/255,60/255,40/255,1
                            min: 0
                            max: 180000
                            step: 1000
                            id: carCost
                        Title4:
                            font_size: "20dp"
                            text: "$" + str(int(carCost.value))

                    CalculatorCategory: # Annual Distance Travelled
                        height: "250dp"
                        BoxLayout:
                            size_hint: 0.65,1
                            pos_hint: {"center_x": .5}
                            Title2:
                                text: "Annual Distance"
                            TooltipMDIconButton:
                                icon: "help-circle-outline"
                                tooltip_text: "Select the approximate distance (in km) you travel by car each year"
                                pos_hint: {"center_x": .5, "center_y": .5}
                        MDSlider:
                            hint: False
                            color: 229/255,60/255,40/255,1
                            min: 0
                            max: 50000
                            step: 500
                            id: annualDistance
                        Title4:
                            font_size: "20dp"
                            text: str(int(annualDistance.value)) + " km"

                BoxLayout:
                    size_hint: 1,None
                    height: "250dp"
                    spacing: "40dp"

                    CalculatorCategory: # Term (length of lease)
                        height: "250dp"
                        BoxLayout:
                            size_hint: 0.55,1
                            pos_hint: {"center_x": .5}
                            Title2:
                                text: "Term Length"
                            TooltipMDIconButton:
                                icon: "help-circle-outline"
                                tooltip_text: "Select your desired term length for the lease"
                                pos_hint: {"center_x": .5, "center_y": .5}
                        GridLayout:
                            cols: 4
                            Check:
                                group: 'lengthTerm'
                                id: term1
                                on_release:
                                    print("12 months", self.active)
                            Check:
                                group: 'lengthTerm'
                                id: term2
                                on_release:
                                    print("24 months", self.active)
                            Check:
                                group: 'lengthTerm'
                                id: term3
                                on_release:
                                    print("36 months", self.active)
                            Check:
                                group: 'lengthTerm'
                                id: term4
                                on_release:
                                    print("48 months", self.active)
                            Title4:
                                text: "12 months"
                            Title4:
                                text: "24 months"
                            Title4:
                                text: "36 months"
                            Title4:
                                text: "48 months"

                    CalculatorCategory: # Car Size
                        height: "250dp"
                        BoxLayout:
                            size_hint: 0.4,1
                            pos_hint: {"center_x": .5}
                            Title2:
                                text: "Car Size"
                            TooltipMDIconButton:
                                icon: "help-circle-outline"
                                tooltip_text: "Select the size of your desired car \n(this is used for fuel calculations)"
                                pos_hint: {"center_x": .5, "center_y": .5}

                        GridLayout:
                            cols: 4
                            Check:
                                group: 'carSize'
                                id: size1
                                on_release:
                                    print("Small", self.active)
                            Check:
                                group: 'carSize'
                                id: size2
                                on_release:
                                    print("Medium", self.active)
                            Check:
                                group: 'carSize'
                                id: size3
                                on_release:
                                    print("Large", self.active)
                            Check:
                                group: 'carSize'
                                id: size4
                                on_release:
                                    print("Sports", self.active)
                            Title4:
                                text: "Small"
                            Title4:
                                text: "Medium"
                            Title4:
                                text: "Large"
                            Title4:
                                text: "Sports"

                CalculatorCategory: # Pre-tax income
                    height: "250dp"
                    size_hint: 1, None
                    BoxLayout:
                        size_hint: 0.15,1
                        pos_hint: {"center_x": .5}
                        Title2:
                            text: "Income"
                        TooltipMDIconButton:
                            icon: "help-circle-outline"
                            tooltip_text: "Select your yearly salary (prior to tax)"
                            pos_hint: {"center_x": .5, "center_y": .5}
                    MDSlider:
                        hint: True
                        color: 229/255,60/255,40/255,1
                        hint_text_color: "white"
                        hint_bg_color: "red"
                        min: 0
                        max: 500000
                        step: 5000
                        id: preTaxIncome
                    Title4:
                        font_size: "20dp"
                        text: "$" + str(int(preTaxIncome.value))

                BoxLayout:
                    size_hint: 1,None
                    height: "300dp"
                    spacing: "40dp"
                    CalculatorCategory: # Incidental
                        height: "300dp"
                        size_hint: 1, None
                        BoxLayout:
                            orientation: "vertical"
                            Title2:
                                text: "Insurance"
                            TooltipMDIconButton:
                                icon: "help-circle-outline"
                                tooltip_text: "Select this option if you would like\ninsurance costs to be included in the lease"
                                pos_hint: {"center_x": .5, "center_y": .5}
                        Image:
                            source: "incidentals/insurance.png"
                        Check:
                            id: inc1
                            on_release:
                                print("insurance", self.active)

                    CalculatorCategory: # Incidental
                        height: "300dp"
                        size_hint: 1, None
                        BoxLayout:
                            orientation: "vertical"
                            Title2:
                                text: "Roadside Assist"
                            TooltipMDIconButton:
                                icon: "help-circle-outline"
                                tooltip_text: "Select this option if you would like\nroadside assistance costs to be included in the lease"
                                pos_hint: {"center_x": .5, "center_y": .5}
                        Image:
                            source: "incidentals/roadside.webp"
                        Check:
                            id: inc2
                            on_release:
                                print("roadside_assist", self.active)

                    CalculatorCategory: # Incidental
                        height: "300dp"
                        size_hint: 1, None
                        BoxLayout:
                            orientation: "vertical"
                            Title2:
                                text: "Tyres"
                            TooltipMDIconButton:
                                icon: "help-circle-outline"
                                tooltip_text: "Select this option if you would like\ntyre costs to be included in the lease"
                                pos_hint: {"center_x": .5, "center_y": .5}
                        Image:
                            source: "incidentals/cleaning.png"
                        Check:
                            id: inc3
                            on_release:
                                print("Tyre", self.active)

                    CalculatorCategory: # Incidental
                        height: "300dp"
                        size_hint: 1, None
                        BoxLayout:
                            orientation: "vertical"
                            Title2:
                                text: "Servicing"
                            TooltipMDIconButton:
                                icon: "help-circle-outline"
                                tooltip_text: "Select this option if you would like\nservicing costs to be included in the lease"
                                pos_hint: {"center_x": .5, "center_y": .5}
                        Image:
                            source: "incidentals/servicing.png"
                        Check:
                            id: inc4
                            on_release:
                                print("servicing", self.active)

                CalculatorCategory:
                    md_bg_color: 229/255,60/255,40/255,.7
                    height: "250dp"
                    size_hint: 1, None
                    Button:
                        text: "CALCULATE"
                        font_name: "font.otf"
                        color: 0,0,0,1
                        bold: True
                        font_size: "50dp"
                        background_color: (0,0,0,0)
                        background_normal: ""
                        canvas.before:
                            Color:
                                rgba: 1,1,1,1
                            RoundedRectangle:
                                size: self.size
                                pos: self.pos
                                radius: [50]
                        on_release:
                            root.calc_values(carCost.value, annualDistance.value, term1.active, term2.active, term3.active, term4.active,
                            size1.active, size2.active, size3.active, size4.active, preTaxIncome.value, inc1.active, inc2.active, inc3.active, inc4.active)

<Manager>:
    name: 'manager'
    Label:
        text: "Loading..."
        color: 'black'

<Title1@Label>:
    font_size: "80dp"
    font_name: "font.otf"
    color: 0,0,0,1

<Title2@Label>:
    font_size: "30dp"
    font_name: "font.otf"
    color: 0,0,0,1

<Title4@Label>:
    font_size: "15dp"
    font_name: "font.otf"
    color: 0,0,0,1

<CalculatorCategory@MDCard>:
    padding: "120dp","30dp"
    radius: [50]
    md_bg_color: 1,1,1,1
    orientation: "vertical"

<Check@MDCheckbox>:
    size_hint: 1, None
    height: "10dp"
    size: "48dp", "48dp"
    selected_color: 229/255,60/255,40/255,1

<Content>:
    cols: 2
    spacing: "10dp"
    size_hint_y: None
    size_hint_x: None
    height: "360dp"
    width: "500dp"

<ItemConfirm>:
    on_release: root.set_icon(check)
    CheckboxLeftWidget:
        id: check
        group: "check"

<SelectionButton>:
    on_release: root.test_fn()